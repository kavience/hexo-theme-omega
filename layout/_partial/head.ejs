<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="google-site-verification" content="xBT4GhYoi5qRD5tr338pgPM5OWHHIDR6mNg1a3euekI" />
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta name="keywords"  content="<%= page.keywords || (config.keyword + ',' + page.title || config.title) %>">
  <meta name="description" content="<%= page.description || page.subtitle || (config.description + '-' + page.title || config.title)  %>">
  <meta property="og:locale" content="<%= config.language  %>">
  <meta property="og:type" content="article">
  <meta property="og:title" content="<%= page.title ? page.title + ' - ' + theme.SEOTitle :  theme.SEOTitle %>">
  <meta property="og:description" content="<%= page.description || page.subtitle || (config.description + '-' + page.title || config.title)  %>">
  <meta property="og:url" content="<%= config.url + config.root + (page.path || '').replace('index.html', '') %>">
  <meta property="og:site_name" content="<%= theme.SEOTitle %>">
  <link rel="shortcut icon" href="<%= config.root %>img/avatar/my-avatar.jpg">
  <title><%= page.title ? page.title + ' - ' + theme.SEOTitle :  theme.SEOTitle %></title>
  <link rel="canonical" href="<%= config.url + config.root + (page.path || '').replace('index.html', '') %>">
  <%- css('css/font-awesome.min') %>
  <%- css('css/bootstrap.min') %>
  <%- css('css/omega')%>
  <%- css('css/highlight') %>
  <%- css('css/gitalk.min')%>
  <%- css('css/kkpager_orange.min')%>

  <%- js('js/jquery.min.js') %>
  <%- js('js/bootstrap.min.js') %>
  <%- js('js/gitalk.min.js') %>
  <%- js('js/kkpager.min.js') %>
  <%- js('js/button.min.js') %>
  <%- js('js/fastclick.min.js') %>
  <%- js('js/omega.js') %>
</head>
